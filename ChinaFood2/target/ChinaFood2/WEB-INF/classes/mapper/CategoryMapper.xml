<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hy.dao.CategoryMapper">
    <insert id="addStyle">
        INSERT INTO f_food_style(food_id,style_id) VALUES(#{foodId},#{styleId})
    </insert>
    <insert id="addScene">
        INSERT INTO f_food_scene(food_id,scene_id) VALUES(#{foodId},#{sceneId})
    </insert>
    <insert id="addTaste">
        INSERT INTO f_food_taste(food_id,taste_id) VALUES(#{foodId},#{tasteId})
    </insert>

</mapper>